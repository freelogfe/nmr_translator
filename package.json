{
  "name": "@freelog/nmr_translator",
  "version": "3.0.0",
  "main": "lib/index.js",
  "dependencies": {
    "antlr4": "4.7.1"
  },
  "scripts": {
    "publish": "npm publish --access=public",
    "build": "cd grammars && java -Xmx500M -cp ../build/antlr-4.7.1-complete.jar org.antlr.v4.Tool -Dlanguage=JavaScript -visitor Mapping.g4 -o ../gen",
    "build2": "java -Xmx500M -cp build/antlr-4.7.1-complete.jar org.antlr.v4.Tool -Dlanguage=JavaScript -visitor grammars/Semver.g4 -o gen",
    "test": "nodemon inspect ./test/index.test.js",
    "test2": "nodemon inspect ./test/semver.test.js"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
